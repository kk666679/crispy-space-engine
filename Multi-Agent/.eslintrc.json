{
    "root": true,
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-jsx/recommended",
        "plugin:jsx-a11y/recommended",
        "prettier"
    ],
    "setting": {
        "react": {
            "version": "detect"
        }
    },
    "overrides": [
        {
            "files": ["*.js", "*.jsx"]
        }
    ]
}